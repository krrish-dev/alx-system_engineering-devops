web01 - CREATE USER slave@'%' IDENTIFIED BY 'password';
web01 - GRANT REPLICATION SLAVE ON *.* TO slave@'%';
web01 - FLUSH PRIVILEGES;
web01 - SHOW MASTER STATUS;

web02 - CHANGE MASTER TO MASTER_HOST='54.85.38.185', MASTER_USER='slave', MASTER_PASSWORD='password', MASTER_LOG_FILE='mysql-bin.000001', MASTER_LOG_POS=749;
web02 - START SLAVE;

sudo service mysql restart



